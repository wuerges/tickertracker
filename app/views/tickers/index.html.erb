<p id="notice"><%= notice %></p>

<h1>Tickers</h1>

<table>
  <thead>
    <tr>
      <th>Ticker</th>
      <th>Description</th>
      <th>Url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tickers.each do |ticker| %>
      <tr>
        <td><%= ticker.ticker %></td>
        <td><%= ticker.description %></td>
        <td><%= ticker.url %></td>
        <td><%= ticker.formated_price %></td>
        <td><%= link_to 'Show', ticker %></td>
        <td><%= link_to 'Edit', edit_ticker_path(ticker) %></td>
        <td><%= link_to 'Destroy', ticker, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ticker', new_ticker_path %>
